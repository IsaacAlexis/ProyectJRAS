<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@drawable/encabezado_mostrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:background="@drawable/pie_usuarios"
        tools:layout_editor_absoluteX="285dp"
        tools:layout_editor_absoluteY="605dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@+id/imageView2" />
    <TextView
        android:id="@+id/labelRegistrarViviendas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Registro de viviendas"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />
    <EditText
        android:id="@+id/CodBarras"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginTop="4dp"
        android:drawableLeft="@drawable/ic_baseline_report_24"
        android:hint="Codigo de Barras"
        android:imeOptions="actionNext"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelRegistrarViviendas" />
    <EditText
        android:id="@+id/txtPropietario"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:drawableLeft="@drawable/ic_baseline_face_24"
        android:hint="Nombre de Propietario"
        android:imeOptions="actionNext"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="427dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CodBarras"/>
    <EditText
        android:id="@+id/txtTelefonoViv"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:drawableLeft="@drawable/ic_baseline_local_phone_24"
        android:hint="Telefono"
        android:imeOptions="actionNext"
        android:inputType="phone"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="486dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPropietario"/>
    <EditText
        android:id="@+id/txtEmail"
        android:layout_width="309dp"
        android:layout_height="43dp"
        android:drawableLeft="@drawable/ic_baseline_email_24"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="533dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTelefonoViv"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <EditText
                android:id="@+id/CalleViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_location_on_24"
                android:hint="Calle de la Vivienda"
                android:imeOptions="actionNext"
                android:inputType="text"
                tools:layout_editor_absoluteX="53dp"
                tools:layout_editor_absoluteY="286dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtEmail"/>
            <EditText
                android:id="@+id/NumViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_location_on_24"
                android:hint="Numero de vivienda"
                android:imeOptions="actionNext"
                android:inputType="number"
                tools:layout_editor_absoluteX="53dp"
                tools:layout_editor_absoluteY="286dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/CalleViv"/>
            <EditText
                android:id="@+id/txtCPViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_pin_drop_24"
                android:hint="C.P."
                android:imeOptions="actionNext"
                android:inputType="number"
                tools:layout_editor_absoluteX="229dp"
                tools:layout_editor_absoluteY="346dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/NumViv"/>
            <EditText
                android:id="@+id/ColViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_location_on_24"
                android:hint="Colonia de vivienda"
                android:imeOptions="actionNext"
                android:inputType="text"
                tools:layout_editor_absoluteX="53dp"
                tools:layout_editor_absoluteY="286dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtCPViv"/>
            <EditText
                android:id="@+id/CiudadViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_location_city_24"
                android:hint="Ciudad"
                android:imeOptions="actionNext"
                android:inputType="text"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="338dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ColViv"/>
            <EditText
                android:id="@+id/EstadoViv"
                android:layout_width="311dp"
                android:layout_height="47dp"
                android:drawableLeft="@drawable/ic_baseline_apartment_24"
                android:hint="Estado"
                android:imeOptions="actionNext"
                android:inputType="text"
                tools:layout_editor_absoluteX="48dp"
                tools:layout_editor_absoluteY="422dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/CiudadViv"/>
            <Button
                android:id="@+id/btnregistrarVivienda"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:background="@drawable/bordes_redondos_azul"
                android:text="Registrar"
                android:textColor="@android:color/black"
                android:textSize="20dp"
                tools:layout_editor_absoluteX="141dp"
                tools:layout_editor_absoluteY="642dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/EstadoViv"/>
        </LinearLayout>
    </ScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>
