<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@drawable/encabezado_mostrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:background="@drawable/pie_usuarios"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/labelUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:text="Registro de Viviendas"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />


    <EditText
        android:id="@+id/txtNoContrato"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="14dp"
        android:drawableLeft="@drawable/ic_baseline_report_24"
        android:hint="No de Contrato"
        android:imeOptions="actionNext"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/txtNombreCompleto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtNombreCompleto"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="13dp"
        android:drawableLeft="@drawable/ic_baseline_accessibility_24"
        android:hint="Nombre Completo"
        android:imeOptions="actionNext"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txtPassUsuarioL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtEstado"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="17dp"
        android:drawableLeft="@drawable/ic_baseline_apartment_24"
        android:hint="Estado"
        android:imeOptions="actionNext"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/txtTelefonoUsuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtCP"
        android:layout_width="143dp"
        android:layout_height="54dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="44dp"
        android:drawableLeft="@drawable/ic_baseline_pin_drop_24"
        android:hint="C.P."
        android:imeOptions="actionNext"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtCiudad"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <EditText
        android:id="@+id/txtCiudad"
        android:layout_width="161dp"
        android:layout_height="54dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="196dp"
        android:layout_marginBottom="18dp"
        android:drawableLeft="@drawable/ic_baseline_location_city_24"
        android:hint="Ciudad"
        android:imeOptions="actionNext"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/txtEstado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <EditText
        android:id="@+id/txtPassUsuarioL"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="13dp"
        android:drawableLeft="@drawable/ic_baseline_location_on_24"
        android:hint="Direccion"
        android:imeOptions="actionNext"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/txtCP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtTelefonoUsuario"
        android:layout_width="311dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="48dp"
        android:drawableLeft="@drawable/ic_baseline_local_phone_24"
        android:hint="Telefono"
        android:imeOptions="actionNext"
        android:inputType="phone"
        app:layout_constraintBottom_toTopOf="@+id/btnregistrarVivienda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="parent" />

    <Button

        android:id="@+id/btnregistrarVivienda"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bordes_redondos_azul"
        android:text="Registrar"
        android:textColor="@android:color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTelefonoUsuario" />

</androidx.constraintlayout.widget.ConstraintLayout>